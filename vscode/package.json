{
	"publisher": "apicatalog",
	"name": "api-catalog",
	"displayName": "apicatalog",
	"description": "",
	"version": "0.0.1",
	"engines": {
		"vscode": "^1.51.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onCommand:APICatalog.start",
		"onView:APICatalog",
		"onWebviewPanel:APICatalog"
	],
	"repository": {
		"type": "git",
		"url": "https://github.com/microsoft/vscode-extension-samples.git"
	},
	"main": "./dist/extension.js",
	"contributes": {
		"commands": [
			{
				"command": "APICatalog.start",
				"title": "Start API Catalog Session",
				"category": "Api Catalog",
				"icon": {
					"light": "media/icon_apicatalog.png",
					"dark": "media/icon_apicatalog.png"
				}
			}
		],
		"viewsContainers": {
			"activitybar": [
				{
					"id": "api-catalog",
					"title": "API Catalog",
					"icon": "media/icon_apicatalog.png"
				}
			]
		},
		"views": {
			"api-catalog": [
				{
					"id": "APICatalog",
					"name": "API Catalog",
					"icon": "media/icon_apicatalog.png",
					"contextualTitle": "API Catalog"
				}
			]
		}
	},
	"scripts": {
		"vscode:prepublish": "npm run package",
		"compile": "webpack --devtool nosources-source-map --config ./build/node-extension.webpack.config.js",
		"watch": "webpack --watch --devtool nosources-source-map --info-verbosity verbose --config ./build/node-extension.webpack.config.js",
		"package": "webpack --mode production --config ./build/node-extension.webpack.config.js",
		"test-compile": "tsc -p ./",
		"test-watch": "tsc -watch -p ./",
		"pretest": "npm run test-compile && npm run lint",
		"lint": "eslint src --ext ts",
		"test": "node ./out/test/runTest.js"
	},
	"devDependencies": {
		"@types/vscode": "^1.51.0",
		"@types/glob": "^7.1.3",
		"@types/mocha": "^8.0.0",
		"@types/node": "^12.11.7",
		"eslint": "^7.9.0",
		"@typescript-eslint/eslint-plugin": "^4.1.1",
		"@typescript-eslint/parser": "^4.1.1",
		"@vue/cli-plugin-typescript": "~4.5.0",
		"@vue/cli-service": "~4.5.0",
		"@vue/eslint-config-typescript": "^5.0.2",
		"glob": "^7.1.6",
		"mocha": "^8.1.3",
		"typescript": "^4.0.2",
		"vscode-test": "^1.4.0",
		"ts-loader": "^8.0.3",
		"webpack": "^4.44.1",
		"webpack-cli": "^3.3.12"
	},
	"dependencies": {
		"core-js": "^3.6.5",
		"vue": "^2.6.11",
		"vue-class-component": "^7.2.3",
		"vue-property-decorator": "^8.4.2"
	}
}
